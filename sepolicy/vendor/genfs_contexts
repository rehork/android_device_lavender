# Battery
genfscon sysfs /devices/soc/c176000.i2c/i2c-2/2-001d            u:object_r:sysfs_battery_supply:s0
genfscon sysfs /module/battery_saver/parameters/enabled u:object_r:sysfs_battery_saver:s0

# Camera
